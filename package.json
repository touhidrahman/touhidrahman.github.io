{
  "name": "touhidrahman-me",
  "version": "11.0.5",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod --baseHref='/touhidrahman.github.io/' && cp CopyCNAME docs/CNAME && cp docs/index.html docs/404.html",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "precommit": "lint-staged",
    "format:ts": "prettier --write 'src/**/*.{js,ts,scss,css,less,jsx,tsx,json}'",
    "format:html": "prettier --write 'src/**/*.{html,xml}'",
    "format:specs": "prettier --write 'e2e/**/*.*'",
    "format": "npm run format:ts && npm run format:html && npm run format:specs",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.5",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~11.0.5",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/router": "~11.0.5",
    "@ngneat/tailwind": "^3.0.6",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "11.0.0-beta.5",
    "@angular-devkit/build-angular": "~0.1100.5",
    "@angular-eslint/builder": "0.8.0-beta.6",
    "@angular-eslint/eslint-plugin": "0.8.0-beta.6",
    "@angular-eslint/eslint-plugin-template": "0.8.0-beta.6",
    "@angular-eslint/schematics": "0.8.0-beta.6",
    "@angular-eslint/template-parser": "0.8.0-beta.6",
    "@angular/cli": "~11.0.5",
    "@angular/compiler-cli": "~11.0.5",
    "@angular/language-service": "~11.0.5",
    "@schematics/angular": "^11.0.5",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "autoprefixer": "10.1.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.8",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "latest",
    "husky": "^4.3.6",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "lint-staged": "^10.5.3",
    "postcss": "8.2.1",
    "postcss-import": "14.0.0",
    "postcss-loader": "4.1.0",
    "postcss-scss": "3.0.4",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "tailwindcss": "2.0.2",
    "ts-node": "9.1.1",
    "typescript": "~4.0.5"
  },
  "lint-staged": {
    "*.{js,ts,scss,css,less,jsx,tsx,json}": "prettier --write",
    "*.{html,xml}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "pre-push": ""
    }
  }
}
